{
  "provider": {
    "name": "bookServiceProvider"
  },
  "consumer": {
    "name": "taobaoServiceConsumer"
  },
  "interactions": [
    {
      "description": "get taobao book info by id",
      "request": {
        "method": "GET",
        "path": "/books/1"
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json"
        },
        "body": {
          "title": "Effective Java",
          "price": 49.49
        },
        "matchingRules": {
          "header": {
            "Content-Type": {
              "matchers": [
                {
                  "match": "regex",
                  "regex": "application/json"
                }
              ],
              "combine": "AND"
            }
          }
        }
      },
      "providerStates": [
        {
          "name": "get taobao book info by id"
        }
      ]
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "3.0.0"
    },
    "pact-jvm": {
      "version": "4.1.7"
    }
  }
}
